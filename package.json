{
  "name": "@kube-js/kube-ts-server",
  "version": "0.0.0-development",
  "description": "Dokerized restful API written in typescript ready to be deployed on kubernetes cluster",
  "main": "index.js",
  "dependencies": {
    "express": "4.16.4",
    "source-map-support": "0.5.10"
  },
  "devDependencies": {
    "@kube-js/semantic-release": "1.0.7",
    "@kube-js/tscommons": "1.0.4",
    "@types/express": "4.16.1",
    "@types/jest": "24.0.9",
    "@types/node": "11.10.5",
    "@types/source-map-support": "0.5.0",
    "jest": "24.3.0",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.0",
    "tslint": "5.13.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.3.3333"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "lint": "tslint --project tsconfig.json --config tslint.json './src/**/*.ts'",
    "test": "jest --coverage",
    "test:watch": "jest --watch --coverage",
    "start": "node ./dist/presenter/express/index.js",
    "semantic-release": "semantic-release-public-circleci-app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kube-js/kube-ts-server.git"
  },
  "keywords": [
    "rest",
    "api",
    "express",
    "typescript",
    "jwt"
  ],
  "author": "Mariusz Rajczakowski <mariuszrajczakowski@pm.me> (http://mariuszrajczakowski.me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kube-js/kube-ts-server/issues"
  },
  "homepage": "https://github.com/kube-js/kube-ts-server#readme",
  "publishConfig": {
    "access": "public"
  }
}
